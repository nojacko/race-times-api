{
  "name": "f1-race-times-web",
  "version": "1.0.0",
  "description": "F1 Race Times Web Server",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "lint": "eslint \"**/*.ts\" --fix",
    "cal:all": "run cal:download; npm run cal:parse; npm run cal:process",
    "cal:download": "ts-node src/scripts/cal-download.ts",
    "cal:parse": "ts-node src/scripts/cal-parse.ts && eslint \"src/data/**/*.ts\" --fix",
    "cal:process": "ts-node src/scripts/cal-process.ts && eslint \"src/data/**/*.ts\" --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
